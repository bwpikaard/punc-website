{
    "require": {
        "erusev/parsedown": "^1.7",
        "erusev/parsedown-extra": "^0.8.1",
        "php-di/php-di": "^6.0",
        "selective/config": "^0.2.0",
        "slim/psr7": "^1",
        "slim/slim": "^4.4",
        "slim/twig-view": "^3.1",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    }
}
